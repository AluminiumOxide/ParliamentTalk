{
  "name": "parliament-talk-server",
  "version": "1.0.0",
  "description": "API for ParliamentTalk democracy tool",
  "main": "index.js",
  "scripts": {
    "build": "babel . --ignore build,coverage,docs,node_modules --out-dir build",
    "prestart": "npm run build && npm-install-changed",
    "start": "node build/index.js",
    "watch": "mocha -R nyan --recursive build/tests/*",
    "pretest": "npm run build && npm-install-changed",
    "test": "mocha --recursive build/tests/unit/*",
    "doc": "jsdoc -d docs/ -r src/*"
  },
  "author": "Sapphyre Gervais",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "hippie-swagger": "^2.0.1",
    "jsonwebtoken": "^7.1.7",
    "mongoose": "^4.5.10",
    "multer": "^1.2.0",
    "promise": "^7.1.1",
    "swagger-parser": "^3.4.1",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "connect": "^3.2.0",
    "istanbul": "^0.4.5",
    "js-yaml": "^3.3.0",
    "jsdoc": "^3.4.0",
    "mocha": "^3.0.2",
    "npm-install-changed": "^1.2.1",
    "sinon": "^1.17.5",
    "swagger-tools": "0.10.1"
  }
}
